{
  "name": "wpck5-react-typescript",
  "version": "1.0.0",
  "description": "A starter template for React with Typescript using Webpack 5",
  "scripts": {
    "dev": "cross-env DEBUG=express:* webpack serve --config webpack/webpack.config.js --env env=dev",
    "clean-install": "rm -r ./node_modules; npm i",
    "build-dll": "webpack --config webpack/webpack.dll.config.js",
    "build-dist": "webpack --config webpack/webpack.config.js --env env=prod",
    "build-full": "webpack --config ./webpack/webpack.dll.config.js && webpack --config webpack/webpack.config.js --env env=prod",
    "start": "node prodServer.js",
    "prod": "webpack --config ./webpack/webpack.dll.config.js && webpack --config webpack/webpack.config.js --env env=prod && node prodServer.js"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Webpack 5",
    "Starter Template",
    "Boilerplate"
  ],
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.16.9",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.4",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-loader": "^8.2.2",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.1.2",
    "less-loader": "^10.0.1",
    "less-vars-to-js": "^1.3.0",
    "mini-css-extract-plugin": "^2.1.0",
    "open": "^8.2.1",
    "prettier": "2.6.2",
    "process": "^0.11.10",
    "react-refresh": "^0.13.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.5",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "5.8.0"
  },
  "peerDependencies": {
